# Copy this file to .env.local for local testing and DO NOT commit real secrets.

# Flask
SECRET_KEY=change-me
PORT=5000

# Z-API (WhatsApp gateway)
# Example ZAPI_URL should point to the exact /send-text endpoint; include token in path if you prefer
# e.g.: https://api.z-api.io/instances/<INSTANCE_ID>/token/<INSTANCE_TOKEN>/send-text
ZAPI_URL=
ZAP_TOKEN=
CLIENT_TOKEN=

# OpenAI
OPENAI_API_KEY=

# Scheduler
SCHED_INTERVAL=60
# Exemplo de .env para uso local
# Copie este arquivo para .env e preencha com seus valores reais (NÃO comite o .env real)

# URL do endpoint (exemplo com /token/<token>/send-text)
ZAPI_URL=

# Token da instância (Bearer)
ZAP_TOKEN=

# Token do cliente, se a sua instância exigir (Client-Token header)
CLIENT_TOKEN=

# Debug (1 para ativar prints detalhados)
DEBUG_ZAPI=0
